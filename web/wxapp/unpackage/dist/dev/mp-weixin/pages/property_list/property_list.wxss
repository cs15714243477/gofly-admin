.list-container {
  height: 100vh;
  background-color: #f6f7f8;
  display: flex;
  flex-direction: column;
}
.header {
  background-color: rgba(255, 255, 255, 0.95);
  -webkit-backdrop-filter: blur(10px);
          backdrop-filter: blur(10px);
  /* TopHeader 自带左右 padding，这里避免叠加 */
  padding: 0 0 16rpx;
  border-bottom: 1rpx solid #e2e8f0;
  z-index: 50;
}
.header .nav-row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 80rpx;
  margin-bottom: 16rpx;
  position: relative;
}
.header .nav-row .nav-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #0f172a;
  flex: 1;
  text-align: left;
  padding-right: 80rpx;
}
.header .nav-row .filter-icon-btn {
  position: absolute;
  right: 0;
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  position: relative;
}
.header .nav-row .filter-icon-btn .material-symbols-outlined {
  font-size: 40rpx;
  color: #0f172a;
}
.header .nav-row .filter-icon-btn:active {
  background-color: #f1f5f9;
}
.header .nav-row .filter-icon-btn.active .material-symbols-outlined {
  color: #2d9cf0;
}
.header .nav-row .filter-dot {
  position: absolute;
  top: 14rpx;
  right: 14rpx;
  width: 14rpx;
  height: 14rpx;
  background-color: #f97316;
  border-radius: 50%;
  border: 3rpx solid rgba(255, 255, 255, 0.95);
}
.header .search-row {
  margin-top: 16rpx;
  margin-bottom: 24rpx;
  padding: 0 16rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  gap: 16rpx;
}
.header .search-row .search-box {
  flex: 1;
  height: 88rpx;
  background-color: #f8fafc;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  padding: 0 24rpx;
  border: 1px solid rgba(226, 232, 240, 0.95);
  box-shadow: 0 10rpx 24rpx rgba(15, 23, 42, 0.06);
}
.header .search-row .search-box .search-icon {
  font-size: 40rpx;
  color: #94a3b8;
}
.header .search-row .search-box .search-input {
  flex: 1;
  margin-left: 16rpx;
  font-size: 28rpx;
  color: #0f172a;
}
.header .category-scroll {
  white-space: nowrap;
  width: 100%;
  padding: 0 16rpx;
  box-sizing: border-box;
}
.header .category-scroll .category-row {
  display: inline-flex;
  gap: 16rpx;
  padding-bottom: 4rpx;
}
.header .category-scroll .category-item {
  padding: 12rpx 32rpx;
  background-color: #ffffff;
  border-radius: 40rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #64748b;
  border: 1px solid #e2e8f0;
  white-space: nowrap;
}
.header .category-scroll .category-item.active {
  background-color: #2d9cf0;
  color: #ffffff;
  border-color: #2d9cf0;
  box-shadow: 0 4rpx 8rpx rgba(45, 156, 240, 0.2);
}

/* TopHeader 右侧按钮：统一尺寸/交互 */
.filter-icon-btn {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  position: relative;
  background-color: transparent;
}
.filter-icon-btn .material-symbols-outlined {
  font-size: 40rpx;
  color: #0f172a;
}
.filter-icon-btn:active {
  background-color: #f1f5f9;
}
.filter-icon-btn.active .material-symbols-outlined {
  color: #2d9cf0;
}
.filter-dot {
  position: absolute;
  top: 14rpx;
  right: 14rpx;
  width: 14rpx;
  height: 14rpx;
  background-color: #f97316;
  border-radius: 50%;
  border: 3rpx solid rgba(255, 255, 255, 0.95);
}
.main-list {
  flex: 1;
  overflow: hidden;
}
.list-content {
  padding: 24rpx 16rpx;
  display: flex;
  flex-direction: column;
  gap: 24rpx;
}
.property-card {
  background-color: #ffffff;
  border-radius: 24rpx;
  overflow: hidden;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.02);
  border: 1px solid #f1f5f9;
}
.property-card.disabled {
  opacity: 0.8;
}
.property-card.disabled .title {
  color: #64748b;
}
.property-card.disabled .price {
  color: #94a3b8;
}
.property-card .card-main {
  padding: 20rpx;
  display: flex;
  gap: 18rpx;
}
.property-card .card-main .image-box {
  width: 224rpx;
  height: 224rpx;
  border-radius: 16rpx;
  overflow: hidden;
  position: relative;
  flex-shrink: 0;
  background-color: #f1f5f9;
}
.property-card .card-main .image-box .property-image {
  width: 100%;
  height: 100%;
}
.property-card .card-main .image-box .image-tag {
  position: absolute;
  top: 8rpx;
  left: 8rpx;
  background-color: rgba(0, 0, 0, 0.6);
  -webkit-backdrop-filter: blur(4rpx);
          backdrop-filter: blur(4rpx);
  color: #ffffff;
  font-size: 20rpx;
  padding: 4rpx 12rpx;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  gap: 4rpx;
}
.property-card .card-main .image-box .image-tag .tag-icon {
  font-size: 20rpx;
}
.property-card .card-main .image-box .mask {
  position: absolute;
  inset: 0;
  background-color: rgba(15, 23, 42, 0.4);
  display: flex;
  align-items: center;
  justify-content: center;
}
.property-card .card-main .image-box .mask .mask-text {
  background-color: rgba(30, 41, 59, 0.8);
  color: #ffffff;
  font-size: 24rpx;
  font-weight: bold;
  padding: 4rpx 16rpx;
  border-radius: 8rpx;
}
.property-card .card-main .image-box .grayscale {
  filter: grayscale(1);
}
.property-card .card-main .info-box {
  flex: 1;
  min-width: 0;
  display: flex;
  flex-direction: column;
  gap: 6rpx;
}
.property-card .card-main .info-box .title {
  font-size: 30rpx;
  font-weight: bold;
  color: #0f172a;
  line-height: 1.3;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  line-clamp: 2;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.property-card .card-main .info-box .meta {
  display: flex;
  align-items: center;
  gap: 12rpx;
  font-size: 24rpx;
  color: #64748b;
}
.property-card .card-main .info-box .meta .bold {
  font-weight: 500;
  color: #334155;
}
.property-card .card-main .info-box .meta .divider {
  color: #e2e8f0;
}
.property-card .card-main .info-box .tags {
  display: flex;
  flex-wrap: wrap;
  gap: 12rpx;
  margin: 2rpx 0;
}
.property-card .card-main .info-box .tags .tag {
  font-size: 20rpx;
  font-weight: 500;
  padding: 4rpx 12rpx;
  border-radius: 8rpx;
  border: 1px solid transparent;
}
.property-card .card-main .info-box .tags .tag.blue {
  background-color: #eff6ff;
  color: #2563eb;
  border-color: #dbeafe;
}
.property-card .card-main .info-box .tags .tag.green {
  background-color: #f0fdf4;
  color: #16a34a;
  border-color: #dcfce7;
}
.property-card .card-main .info-box .tags .tag.orange {
  background-color: #fff7ed;
  color: #ea580c;
  border-color: #ffedd5;
}
.property-card .card-main .info-box .tags .tag.indigo {
  background-color: #eef2ff;
  color: #4f46e5;
  border-color: #e0e7ff;
}
.property-card .card-main .info-box .tags .tag.purple {
  background-color: #faf5ff;
  color: #9333ea;
  border-color: #f3e8ff;
}
.property-card .card-main .info-box .tags .tag.grey {
  background-color: #f8fafc;
  color: #64748b;
}
.property-card .card-main .info-box .price-row {
  display: flex;
  align-items: baseline;
  gap: 16rpx;
  margin-top: auto;
}
.property-card .card-main .info-box .price-row .price {
  font-size: 36rpx;
  font-weight: bold;
  color: #f97316;
}
.property-card .card-main .info-box .price-row .unit-price {
  font-size: 20rpx;
  color: #94a3b8;
}
.property-card .card-main .info-box .stats {
  font-size: 20rpx;
  color: #94a3b8;
  display: flex;
  align-items: center;
}
.property-card .card-main .info-box .stats .stats-sep {
  width: 1px;
  height: 16rpx;
  background-color: #e2e8f0;
  margin: 0 12rpx;
}
.property-card .card-footer {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 14rpx 20rpx;
  border-top: 1rpx solid transparent;
}
.property-card .card-footer.orange-footer {
  background-color: #fff7ed;
  border-color: #ffedd5;
}
.property-card .card-footer.orange-footer .footer-icon-box {
  background-color: #f97316;
}
.property-card .card-footer.orange-footer .footer-text {
  color: #9a3412;
  font-weight: bold;
}
.property-card .card-footer.grey-footer {
  background-color: #f8fafc;
  border-color: #f1f5f9;
}
.property-card .card-footer.grey-footer .footer-icon-box {
  background-color: #cbd5e1;
}
.property-card .card-footer.grey-footer .footer-text {
  color: #64748b;
  font-weight: 500;
}
.property-card .card-footer .footer-left {
  display: flex;
  align-items: center;
  gap: 16rpx;
  flex: 1;
  min-width: 0;
}
.property-card .card-footer .footer-left .footer-icon-box {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
}
.property-card .card-footer .footer-left .footer-icon-box .footer-icon {
  color: #ffffff;
  font-size: 28rpx;
}
.property-card .card-footer .footer-left .footer-text {
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.property-card .card-footer .footer-btn {
  height: 52rpx;
  padding: 0 24rpx;
  background-color: #ffffff;
  border: 1px solid #fed7aa;
  border-radius: 40rpx;
  display: flex;
  align-items: center;
  gap: 8rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #ea580c;
  line-height: 1;
  margin-left: 24rpx;
}
.property-card .card-footer .footer-btn::after {
  border: none;
}
.property-card .card-footer .footer-btn .btn-icon {
  font-size: 28rpx;
}
.property-card .card-footer .footer-btn:active {
  background-color: #fff7ed;
  transform: scale(0.95);
}
.property-card .card-footer .footer-btn-disabled {
  height: 52rpx;
  padding: 0 24rpx;
  background-color: #ffffff;
  border: 1px solid #e2e8f0;
  border-radius: 40rpx;
  font-size: 22rpx;
  color: #94a3b8;
  margin-left: 24rpx;
}
.property-card .card-footer .footer-btn-disabled::after {
  border: none;
}
.tab-bar {
  background-color: #ffffff;
  border-top: 1rpx solid #e2e8f0;
  padding: 16rpx 48rpx calc(env(safe-area-inset-bottom) + 16rpx);
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  z-index: 100;
}
.tab-bar .tab-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 8rpx;
  color: #94a3b8;
}
.tab-bar .tab-item .tab-icon {
  font-size: 52rpx;
}
.tab-bar .tab-item .tab-icon.fill {
  font-variation-settings: "FILL" 1;
}
.tab-bar .tab-item .tab-text {
  font-size: 20rpx;
  font-weight: 500;
}
.tab-bar .tab-item.active {
  color: #2d9cf0;
}
.tab-bar .add-btn-container {
  position: relative;
  top: -40rpx;
}
.tab-bar .add-btn-container .add-btn {
  width: 112rpx;
  height: 112rpx;
  background-color: #2d9cf0;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 8rpx 20rpx rgba(45, 156, 240, 0.4);
}
.tab-bar .add-btn-container .add-btn .material-symbols-outlined {
  color: #ffffff;
  font-size: 48rpx;
}
.tab-bar .add-btn-container .add-btn:active {
  transform: scale(0.9) translateY(4rpx);
}
.bottom-spacer {
  height: 160rpx;
}
.placeholder {
  color: #94a3b8;
}

/* 筛选弹层（避免与卡片内 .mask 冲突，单独命名） */
.filter-mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(15, 23, 42, 0.45);
  z-index: 999;
  display: flex;
  align-items: flex-end;
}
.filter-sheet {
  width: 100%;
  background-color: #ffffff;
  border-radius: 32rpx 32rpx 0 0;
  padding: 24rpx 24rpx calc(env(safe-area-inset-bottom) + 24rpx);
  box-sizing: border-box;
}
.filter-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 8rpx;
}
.filter-title {
  font-size: 30rpx;
  font-weight: 800;
  color: #0f172a;
}
.filter-reset {
  font-size: 26rpx;
  color: #64748b;
}
.filter-section {
  padding-top: 18rpx;
}
.filter-section-title {
  font-size: 26rpx;
  font-weight: 700;
  color: #334155;
  margin-bottom: 12rpx;
  display: block;
}
.chip-row {
  display: flex;
  flex-wrap: wrap;
  gap: 16rpx;
}
.chip {
  padding: 12rpx 22rpx;
  border-radius: 40rpx;
  background-color: #f1f5f9;
  color: #334155;
  font-size: 24rpx;
  border: 1px solid transparent;
}
.chip.active {
  background-color: rgba(45, 156, 240, 0.12);
  color: #2d9cf0;
  border-color: rgba(45, 156, 240, 0.25);
  font-weight: 700;
}
.radio-list {
  border-radius: 20rpx;
  overflow: hidden;
  border: 1px solid #f1f5f9;
}
.radio-item {
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 18rpx;
  border-bottom: 1rpx solid #f1f5f9;
  color: #334155;
}
.radio-item:last-child {
  border-bottom: none;
}
.radio-item.active {
  color: #2d9cf0;
  font-weight: 700;
}
.radio-check {
  font-size: 36rpx;
}
.filter-footer {
  display: flex;
  gap: 16rpx;
  margin-top: 20rpx;
}
.filter-btn {
  flex: 1;
  height: 80rpx;
  border-radius: 20rpx;
  font-size: 28rpx;
  font-weight: 800;
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 1;
  border: none;
}
.filter-btn::after {
  border: none;
}
.filter-btn.ghost {
  background-color: #f1f5f9;
  color: #334155;
}
.filter-btn.primary {
  background-color: #2d9cf0;
  color: #ffffff;
}